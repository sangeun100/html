<meta charset="UTF-8">
<script>
$(document).ready(function () {
	var imgs = ['img/a1.jfif','img/a2.jfif','img/a3.jfif'];
	var cnt = 0;
	setInterval(function () { 
		$('#image').attr('src',imgs[cnt % imgs.length]);
		$('h1').text(cnt);
		cnt++;
		}, 1000);
});
</script>
<h1>GOODS </h1>
<img id="image">
