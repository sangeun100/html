font-family: 'Poor Story', cursive;
}
</style>
<script>
$(document).ready(function () {
	var imgs = ['img/a1.jfif','img/a2.jfif','img/a3.jfif'];
	var cnt = 0;
	setInterval(function () { 
		$('#image').attr('src',imgs[cnt % imgs.length]);
		$('#image').text(cnt);
		cnt++;
		}, 1000);
});
</script>
<h1>GOODS(광고) </h1>
<img id="image">
